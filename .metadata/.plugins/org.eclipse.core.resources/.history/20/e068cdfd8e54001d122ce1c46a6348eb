package cl.generation.f20221025;

public class EjercicioGrupoArreglos {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] numeros = new int[6];
		numeros[0]=1;
		numeros[1]=2;
		numeros[2]=3;
		numeros[3]=4;
		numeros[4]=5;
		numeros[5]=6;
		
		
		int i=0;
		
		while (numeros[i]!=5) {
			
			i++;
			
		}
		System.out.println(i);

	}

}
