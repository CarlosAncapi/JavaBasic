package cl.generation.f20221102;

public class Basquebolista extends EquipoBasquetbol {
	
	private Float peso;
	private Float altura;
	
	public Basquebolista() {
		super();
	}

	public Basquebolista(Float peso, Float altura) {
		super();
		this.peso = peso;
		this.altura = altura;
	}

	public Float getPeso() {
		return peso;
	}

	public void setPeso(Float peso) {
		this.peso = peso;
	}

	public Float getAltura() {
		return altura;
	}

	public void setAltura(Float altura) {
		this.altura = altura;
	}
	/*
	@Override
	public String toString() {
		return "Basquebolista [peso=" + peso + ", altura=" + altura + ", getNombre()=" + getNombre()
				+ ", getApellido()=" + getApellido() + ", getEdad()=" + getEdad() + "]";
	}
	*/
	

}
