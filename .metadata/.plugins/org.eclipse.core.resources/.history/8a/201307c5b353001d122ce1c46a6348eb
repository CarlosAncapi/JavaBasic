package cl.generation.f20221024;

public class Arreglos {

	public static void main(String[] args) {
		
		//Arreglos(array) en Java (estaticos no cambia de tama√±o)
		int[] arreglo = new int [3];
		
		//arreglo datos
		arreglo[0]=4;
		arreglo[1]=5;
		arreglo[2]=6;
		//arreglo[3]=8;
		
		System.out.println(arreglo.toString());
		

	}

}
