package cl.generation.f20221027;

public class UsoObjeto {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//instancia de clase
		Auto auto1= new Auto();
		
		//Se asignan valores a traves del metodo setter
		auto1.setMarca("Nissan");
		auto1.setColor("Rojo");
		
		//verificar el valor ingresado
		System.out.println(auto1.toString());
		
		//consultar el contenido de los atributos
		
		String colorsito =auto1.getColor();
		System.out.println(colorsito);
		
		String modelito=auto1.getModelo();
		System.out.println(modelito);

	}

}
