		alumno1.setApellido("perez");
		alumno1.setEdad(15);
		alumno1.setCurso("segundo");
		
		array_alumnos.add(alumno1);
		
		
		Profesor profesor1 = new Profesor();
		
		ArrayList<Profesor> listaProfesores = new ArrayList<Profesor>();
		
		profesor1.setNombre("pedrito");
		profesor1.setApellido("perez");
		profesor1.setEdad(15);
		profesor1.setAniosExperiencia(10);
		profesor1.setAsignatura("Lenguaje");
		
		
		array_alumnos.add(profesor1);
		
		
		
		ArrayList<Directivos> listaDirectivos = new ArrayList<Directivos>();


	}

}
