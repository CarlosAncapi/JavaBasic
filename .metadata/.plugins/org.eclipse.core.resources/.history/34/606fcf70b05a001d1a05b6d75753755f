package cl.generation.f20221102;

public class Main {

	public static void main(String[] args) {
		// herencia en java
		//instanciando objetos
		
		Entrenador entrenador = new Entrenador();
		entrenador.setNombre("Jhon");
		entrenador.setApellido("Doe");
		entrenador.setEdad(55);
